      const incomingData = response.map(item => ({
                        date: item[0].qText,
                        value1: parseFloat((item[1].qText).replace('%', '').trim()),
                        value2: parseFloat((item[2].qText).replace('%', '').trim()),
                        value1Text:item[1].qText,
                        value2Text:item[2].qText

                    }));
