const handleClick = (value) => {
  setSelectedOption(value);
};

useEffect(() => {
  if (selectedOption === 'Bolge') {
    setTableObjectId('CvdJs');
  } else {
    setTableObjectId('kyFDGfJ');
  }
}, [selectedOption]); // selectedOption değiştiğinde tetiklenir

<TableChart key={tableObjectId} app={app} objectId={tableObjectId} onLoaded={() => handleChildLoaded('child1')}/>
