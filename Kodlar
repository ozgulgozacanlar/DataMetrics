const filteredApps = qlikTableArray.flatMap(item =>
  item.apps.filter(app => {
    const normalizedAppName = app.appName.replace(/\s+/g, '').toLowerCase(); // Boşlukları kaldırıp küçük harfe çeviriyoruz
    const normalizedSearchTerm = searchTerm.replace(/\s+/g, '').toLowerCase(); // Aynı şekilde arama terimini normalize ediyoruz
    
    return isMatch(normalizedAppName, normalizedSearchTerm) && normalizedAppName.includes(normalizedSearchTerm);
  })
);
