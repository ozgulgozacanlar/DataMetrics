const xAxisGroup = svg.append("g")
  .attr("class", "x-axis") // Burada .x-axis sınıfını ekliyoruz
  .attr("transform", `translate(0,${height})`)
  .call(xAxis)
  .selectAll('.domain')
  .remove();

// X eksenindeki tick'lerin line'larını kırmızı yap
xAxisGroup.selectAll('.tick line')           
  .attr("stroke", "red");
