export function openApp(appId) {
  return new Promise((resolve, reject) => {
    qlikAPI.then((qlik) => {
      // App'i açmaya çalışırken hata alırsak, burada yakalayabiliriz
      resolve(qlik.openApp(appId, config)); // App başarıyla açılırsa
    }).catch((error) => {
      reject(error); // Eğer hata oluşursa, bunu dışarıya ilet
    });
  });
}
