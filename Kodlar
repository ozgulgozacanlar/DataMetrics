yAxis
  .selectAll(".tick text")
  .style("visibility", "hidden");  // TÃ¼m metinleri gizliyoruz



    barsGroup
      .selectAll("rect")
      .data((d) => d.slice(1))
      .enter()
      .append("rect")
      .attr("x", (_, i) => x1(i))
      .attr("y", (d) => y(d.qNum))
      .attr("width", x1.bandwidth())
      .attr("height", (d) => height - y(d.qNum))
      .attr("fill", (_, i) => (i % 2 === 0 ? "#2c84cc" : "#a4c48c"));
