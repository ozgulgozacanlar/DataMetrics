export function openApp(appId) {
  return new Promise((resolve, reject) => {
    qlikAPI.then((qlik) => {
      try {
        const app = qlik.openApp(appId, config);
        resolve(app);  // Uygulama başarılı şekilde açılırsa çözülür
      } catch (error) {
        console.error('openApp çağrılırken hata:', error);  // Hata detaylarını konsola yazdırıyoruz
        reject(error);  // Hata burada reddedilir
      }
    }).catch((error) => {
      console.error('qlikAPI ile ilgili hata:', error);  // qlikAPI hatasını loglarız
      reject(error); // `qlikAPI`'den gelen hata
    });
  });
}
