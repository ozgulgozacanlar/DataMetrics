    console.log(`Tıklanan başlık: ${cellText}`);
    
    app.field("PROTOKOL_NUMARASI").selectValues([Number(cellText)]).then(() => {
      console.log(`parametresi seçil: ${cellText}`);

      console.log(`parametresi seçil: ${Number(cellText)}`);
      console.log(app.selectionState().selections)
      modalChange(true);

    })

  };
