const getAllRows = (rows, parentId = '') => {
  let result = [];
  
  rows.forEach((row) => {
    const key = parentId ? `${parentId}-${row.id}` : `${row.id}`;
    result.push(key); // Ana satır ekleniyor
    
    if (row.children && row.children.length > 0) {
      // Eğer çocuk varsa, alt satırları ve onların alt satırlarını da açmak için recursive çağrı
      result = result.concat(getAllRows(row.children, key));
    }
  });

  return result;
};
