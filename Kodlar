 const onlyAppNames = qlikTableArray
      .flatMap(item => item.apps)
      .filter(app => app.appName)
      .map(app => ({
        appName: app.appName,
        appId: app.appId,
        isFav: favoriteArray.includes(app.appName) // Favori olup olmadığını kontrol ediyoruz
      }));

 <StreamCard 
          key={appIndex} 
          title={renderHighlightedText(app.appName, searchTerm)} 
          svg={getRandomIcon()} 
          id={app.appId} 
          cardType={"app"} 
          color={getRandomColor()} 
          isFav={app.isFav} // `isFav` bilgisini burada ilet
        />
