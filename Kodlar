       {pageName.includes("detay") ? (
          <>
             <div className='mt-2'>
            <button 
            onClick={handleClickBack}
            className='back-button'
          >
            <span className='back-arrow'></span>
            |
          </button>

            </div>
          </>
        ) : (
          <div className="logo-wrapper"  onClick={() => handleClickPage("/")}><GarantiLogo /></div>
        )}
