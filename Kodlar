   <div className='favorite-views'>
                  <CarouselWrapper>
                    {(favorites && favorites.length != undefined ? favorites.map((item, i) => <FavoriteCard key={i} title={item.name} date={item.publishTime} thunmnail={item.thunmnail} description={item.description} id={item.id} onToggle={handleAddRemoveFavoriteApp}/>) : <></>)}
                  </CarouselWrapper>
                </div>
