catch (error) {
    const result = handleError(error); // handleError çağrıldığında 403 hatasında 'Forbidden' dönecek
    if (result === 'Forbidden') {
      return 'Forbidden'; // Eğer 'Forbidden' dönerse, bu durumu belirleyelim
    }
    // Diğer hatalarda, error'u yeniden fırlatalım
    throw error;  
  }
