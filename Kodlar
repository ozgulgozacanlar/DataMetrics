const initialState = {
  navClicked: false,
  favoriteChange:0

};

export const navbarSlice = createSlice({
  name: 'navbar',
  initialState,
  reducers: {
    setClick: (state, action) => {
      state.navClicked = action.payload;
    },
    setFavoriteChange: (state, action) => {
      state.favoriteChange += action.payload;
    }
  },
});
