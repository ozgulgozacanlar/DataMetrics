export async function openApp(appId) {
  try {
    const qlik = await qlikAPI;  // qlikAPI'yi çöz
    const app = await qlik.openApp(appId, config);  // openApp çağrısını bekle
    return app;  // Başarıyla çözülürse uygulama döndürülür
  } catch (error) {
    console.error('Uygulama açılırken hata oluştu:', error);
    throw error;  // Hata fırlatılır, böylece catch ile yakalanabilir
  }
}
