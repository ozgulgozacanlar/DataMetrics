const firstArray = [header[0], ...datas.map(item => item[0].qText)];
  // İkinci array oluşturuluyor
  const secondArray = header.slice(1).map((headerItem, idx) => {
    return {
      [headerItem]: datas.map(item => item[idx + 1].qText)
    };
  });

  console.log("İlk Array:", firstArray);
  console.log("İkinci Array:", secondArray);
