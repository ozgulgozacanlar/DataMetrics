  <div className="app-card-wrapper">
    {favorites && favorites.length > 0 ? (
      favorites.map((item, i) => (
        <AppCard
          key={i}
          title={item.name}
          date={item.publishTime}
          thunmnail={item.thunmnail}
          description={item.description}
          id={item.id}
          favStatu={item.favStatus}
        />
      ))
    ) : (
      <div className="no-favorites-message">Favori bulunamadÄ±</div>
    )}
  </div>
