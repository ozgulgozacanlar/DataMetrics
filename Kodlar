  useEffect(() => {
     const filteredApps = qlikTableArray.flatMap(item =>
       item.apps.filter(app => isMatch(app.appName, searchTerm))
     );

    console.log("filteredApps with search and favorites match", filteredApps);
    //Eşleşen app'ler sadece arama terimiyle tam olarak eşleşenleri alacak şekilde filtrelenir
     const exactFilteredApps = filteredApps.filter(app =>
      app.appName.toLowerCase().includes(searchTerm.toLowerCase())
     );
    


     
    setFilteredApps(exactFilteredApps);
  }, [searchTerm, qlikTableArray,favorites]);
