// Orta kısımlardaki tick'leri eşit aralıklarla ayarlayalım
// Yalnızca ilk ve son değeri manuel olarak ekleyip, aradaki değerleri otomatik olarak bırakıyoruz.
const tickStep = (lastTick - firstTick) / (tickCount - 1); // Aralık hesaplama
const autoTicks = d3.range(firstTick + tickStep, lastTick, tickStep);  // Eşit aralıklarla tick'ler oluşturma

// Tüm tick'leri birleştirelim
const customTicks = [firstTick, ...autoTicks, lastTick];

console.log(customTicks);
