getObjectData(app, "NvMRCHa").then((response) => {
    if (!response) {
        console.error('HorizontalBarChartwithVal Component : response verisi boş veya tanımsız.');
        return;
    }

    // response[0][0].qText'inden | işaretinden sonraki kısmı almak
    const qTextPart = response[0][0].qText.split('|')[1]?.trim(); // | işaretinden sonrası, boşlukları temizle

    // Eğer qTextPart varsa
    if (qTextPart) {
        // incomingData üzerinde eşleştirme yapma
        const matchingIndex = incomingData.findIndex(item => item.name === qTextPart);
        
        // Eğer eşleşen bir veri varsa, o veriyi array'in başına taşı
        if (matchingIndex !== -1) {
            const matchingItem = incomingData.splice(matchingIndex, 1)[0]; // eşleşen veriyi çıkar
            incomingData.unshift(matchingItem); // başa ekle
        }
    }

    console.log("Updated incomingData:", incomingData);
    setData(incomingData);
    setTimeout(() => { onLoaded(); }, 250);

}).catch((error) => {
    console.error('HorizontalBarChartwithVal Component : getObjectData() methodu veri alınırken hata oluştu:', error);
});
