  const handleFootNote = (footnoteText) => {
    console.log(footnoteText)
    setFootNoteText(footnoteText);
  }

                <CardTitle app={app} objectId={'286328fb-e24c-4a23-b937-c9f0b568407e'} getFootnoteText = {handleFootNote}/>


export default function CardTitle({app , objectId,getFootnoteText}) {

    const [cardTitle, setCardTitle] = useState("");

    useEffect(() => {
        function renderQlikTableObject(objectId) {
          if (app) {          
            app.getObject(objectId).then((model) => {

              let titles = model.layout.title.trim();
              if(titles === ""){
                if(model.genericType == "table"){
                  titles = model.layout.qHyperCube.qDimensionInfo[0].qFallbackTitle;
               }
               else if(model.genericType == "linechart"){
                  titles = model.layout.title;
               }
               else if(model.genericType== "action-button"){
                 titles = model.layout.style.label;
               }
              }          
              setCardTitle(capitalizeFirstLetter(titles));

              if(model.layout.footnote !== ""){
                getFootnoteText(model.layout.footnote);
              }
            });
          } else {
            // console.error('Qlik Sense app is not loaded yet');
          }
        }
        renderQlikTableObject(objectId);
      }, [app, objectId]);
  
return (
    <div className="card-title">{cardTitle.toUpperCase()}</div>

  )
}
