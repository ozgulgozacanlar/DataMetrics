const tickInterval = 10;

// Y ekseninin minimum ve maksimum değerlerini belirleyelim
const yMin = Math.floor(d3.min(data));  // Verilerin en küçük değeri
const yMax = Math.ceil(d3.max(data));  // Verilerin en büyük değeri

// Sabit aralıklarla tick değerlerini oluşturuyoruz
const yTicks = d3.range(yMin, yMax, tickInterval);
