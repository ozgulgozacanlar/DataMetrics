 // Veri setini değiştirme (örneğin d.name veya diğer parametre ile güncelleme)
    const updatedData = data.map(item => {
        // Burada istenen filtreleme veya değişiklik yapılabilir
        if (item.name === clickedValue) {
            // Belirli item'ı değiştir
            item.value = newValue; // örneğin yeni bir değeri buraya ekleyebilirsiniz
        }
        return item;
    });

    // Yeni veriyi setData ile güncelliyoruz
    setData(updatedData); // Burada data'yı güncelliyoruz
