const [loadedCount, setLoadedCount] = useState(0);
const [loadedComponents, setLoadedComponents] = useState([]);  // Yüklenen bileşenleri sakla

const handleDataLoaded = (componentId) => {
  if (!loadedComponents.includes(componentId)) {
    setLoadedCount(prevCount => prevCount + 1);
    setLoadedComponents(prev => [...prev, componentId]);  // Yüklenen bileşeni kaydet
  }
};

<TableWithPoint
  app={app}
  objectId={"gWQgq"}
  page={"gosas"}
  onDataLoaded={() => handleDataLoaded('table1')}  // Bileşenin ID'sini geçir
/>
